<h1>検索結果</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>area</th></th>
      <th>count</th>
      <th>genre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |team| %>
      <tr>
        <td><%= team.name %></td>
        <td><%= team.area %></td>
        <td><%= team.count %></td>
        <td><%= team.genre %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'チーム一覧', group_teams_path %>